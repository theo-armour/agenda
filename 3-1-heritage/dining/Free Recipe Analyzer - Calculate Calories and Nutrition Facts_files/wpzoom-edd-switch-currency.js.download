jQuery(document).ready(function($){var existingCountryIso=Cookies.get('wpzoom_edd_user_country');if(existingCountryIso===undefined){$.ajax({url:'https://geo.easydigitaldownloads.com/v3/geolocate/json',type:'GET',dataType:'json',success:function(response){if(response&&response.country_iso){var userCountryIso=response.country_iso;Cookies.set('wpzoom_edd_user_country',userCountryIso,{expires:1/24,path:'/'});console.log('User Country ISO: '+userCountryIso)}else{console.error('Unable to fetch user country.')}},error:function(jqXHR,textStatus,errorThrown){console.error('AJAX Error: '+textStatus,errorThrown)}})}else{console.log('User Country ISO already exists:',existingCountryIso)}})