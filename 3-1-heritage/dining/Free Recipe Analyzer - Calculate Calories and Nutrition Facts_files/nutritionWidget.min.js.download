function spoonacularShowNutritionComposition(e,nutrient){var offsetX=100;var offsetY=-5;var identifier="spoonacularNutritionComposition-"+nutrient;if(document.getElementById(identifier)!=undefined){var nc=$("#"+identifier);nc.show();nc.attr("style","position:absolute");nc.offset({top:(e.pageY+offsetY),left:(e.pageX+offsetX)});var c=eval("chart"+nutrient);c.render()}}function spoonacularHideNutritionComposition(a){var b="spoonacularNutritionComposition-"+a;if(document.getElementById(b)!=undefined){var c=$("#"+b);c.hide()}};